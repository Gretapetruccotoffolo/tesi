import pandas as pd

# Carica il file
df = pd.read_csv("swept_area.csv")

# Filtra solo i valori > 0
groundspeed_pos = df[df["GroundSpeed"] > 0]["GroundSpeed"]
wingspread_pos = df[df["Cal_WingSpread"] > 0]["Cal_WingSpread"]

# Calcola le medie
media_groundspeed = groundspeed_pos.mean()
media_wingspread = wingspread_pos.mean()

# Stampa i risultati
print(f"✅ Media Groundspeed (> 0): {media_groundspeed:.2f}")
print(f"✅ Media Cal_WingSpread (> 0): {media_wingspread:.2f}")
